{
    "name": "novaway/open-graph-bundle",
    "description": "Provide a simple integration of the OpenGraph protocol",
    "type": "symfony-bundle",
    "keywords": ["open graph", "facebook", "symfony", "bundle"],
    "homepage": "http://github.com/novaway/OpenGraphBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jérémy DECOOL",
            "email": "jeremy@novaway.fr"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "symfony/symfony": "~2.3 || ~3.0 || ^4.0",
        "novaway/open-graph": "^1.0"
    },
    "require-dev": {
        "atoum/atoum": "^2.5 || ^3.0",
        "atoum/stubs": "^2.5"
    },
    "autoload": {
        "psr-4": {"Novaway\\Bundle\\OpenGraphBundle\\": ""},
        "exclude-from-classmap": ["Tests/"]
    }
}
